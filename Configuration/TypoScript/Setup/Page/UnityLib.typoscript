[global]

[applicationContext = Development*]
    lib.urlValue = TEXT
    lib.urlValue.value = http://cms8stage.hark-shop.de/
[end]

[applicationContext = Development*] && [globalString = _SERVER|HTTPS=on]
  lib.urlValue.value = https://cms8stage.hark-shop.de/
[global]