# lib.urlValue can be used anywhere in the extensions where the url from TYPO3 instance is required.
lib.urlValue = TEXT
lib.urlValue.value = http://www.hark.de/

lib.magurlValue = TEXT
lib.magurlValue.value = https://m2stage.hark-shop.de/

[globalString = _SERVER|HTTPS=on]
